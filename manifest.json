{
    "manifest_version": 3,
    "name": "Zekr Reminder",
    "version": "1.0.0",
    "description": "Zekr Reminder, is a chrome extension to remind you to recite andy random Zekr during your time on the internet",
    "background": {
        "service_worker": "./background.js"
    },
    "permissions": [
        "notifications",
        "alarms"
    ],
    "author": "nagwan",
    "host_permissions": [],
    "action": {
        "default_popup": "pop-up.html",
        "default_title": "Zekr Reminder",
        "default_icon": {
            "16": "/assets/images/logo.png",
            "32": "/assets/images/logo.png",
            "48": "/assets/images/logo.png",
            "128": "/assets/images/logo.png"
        }
    },
    "icons": {
        "16": "/assets/images/logo.png",
        "32": "/assets/images/logo.png",
        "48": "/assets/images/logo.png",
        "128": "/assets/images/logo.png"
    }
}