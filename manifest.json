{
    "manifest_version": 3,
    "name": "وَذْكَر",
    "version": "1.0.0",
    "description": "وَذْكَر هي إضافة تعطر لسانك و تهذبه بذكر الله عن طريق إرسال إشعارات بالأذكار الصباحية والمسائية لتجعل لسانك دائما علي ذكر الله ",
    "background": {
    "service_worker": "background.js",
    "type": "module"
    },
    "permissions": [
    "notifications",
    "alarms",
    "storage"
    ],
    "options_ui": {
    "page": "options.html",
    "open_in_tab": false,
    "type": "module"
    },
    "author": "Nagwan",
    "action": {
    "default_popup": "pop-up.html",
    "default_title": "وَذْكَر",
    "default_icon": {
    "16": "/assets/images/tasbih.png",
    "32": "/assets/images/tasbih.png",
    "48": "/assets/images/tasbih.png",
    "128": "/assets/images/tasbih.png"
    }
    },
    "icons": {
    "16": "/assets/images/tasbih.png",
    "32": "/assets/images/tasbih.png",
    "48": "/assets/images/tasbih.png",
    "128": "/assets/images/tasbih.png"
    }
    }
    
    